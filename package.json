{
  "name": "diceware-generator",
  "version": "1.2.0",
  "description": "A diceware generator",
  "main": "dist",
  "scripts": {
    "prebuild": "npm run lint -s && npm run clean -s",
    "build": "babel src --out-dir dist --source-maps",
    "lint": "eslint src",
    "clean": "rimraf dist",
    "prepublish": "npm run test -s",
    "pretest": "npm run build -s",
    "test": "mocha --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lgaticaq/diceware-generator.git"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "keywords": [
    "diceware",
    "password",
    "generator"
  ],
  "author": "Leonardo Gatica <lgatica@protonmail.com> (https://about.me/lgatica)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lgaticaq/diceware-generator/issues"
  },
  "homepage": "https://github.com/lgaticaq/diceware-generator#readme",
  "dependencies": {
    "diceware-wordlist-en": "^1.0.0",
    "diceware-wordlist-jp": "^1.0.1",
    "diceware-wordlist-sp": "^1.0.0",
    "diceware-wordlist-swe": "^1.0.1",
    "lodash": "^3.10.1",
    "secure-random": "^1.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "js-combinatorics": "^0.5.0",
    "mocha": "^2.3.4",
    "rimraf": "^2.4.4"
  },
  "eslintConfig": {
    "rules": {
      "indent": [
        2,
        2
      ],
      "quotes": [
        2,
        "single"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ]
    },
    "ecmaFeatures": {
      "modules": true
    },
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "extends": "eslint:recommended"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
